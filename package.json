{
  "name": "clickson-api",
  "version": "1.0.0",
  "description": "API du projet clickson",
  "main": "index.js",
  "author": "ABC Transition Bas Carbone",
  "license": "MIT",
  "scripts": {
    "vercel": "vercel dev -l 4000",
    "vercel:dev": "dotenv -e .env.development vercel dev -l 4000",
    "swagger-gen": "node swagger.js",
    "open-browser": "start http://localhost:4000/doc",
    "migrate:generate": "dotenv -e .env.development -- npx prisma generate",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "migrate:seed": "dotenv -e .env.development -- npx prisma db seed",
    "migrate:staging": "npx prisma migrate deploy",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@vercel/postgres": "^0.8.0",
    "cors": "^2.8.5",
    "express": "^4.19.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@prisma/client": "^5.21.1",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "prisma": "^5.21.1",
    "prisma-merge": "^0.2.0",
    "supertest": "^7.0.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5",
    "vercel": "^37.14.0"
  }
}
